{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "jailbreak_diffusion.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/jailbreak_diffusion.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "test_diffusion_model.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_diffusion_model.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "exp.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            }
        },
        {
            "name": "DACA exp.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--config_path",
                "config/DACA/stable-diffusion-xl-base-0.9_DACA.yaml"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "7"
            },
        },
        {
            "name": "PGJ exp_direct_attack_prompt.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/exp_direct_attack_prompt.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--config_path",
                "config/PGJ/stable-diffusion-xl-base-0.9_PGJ.yaml"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "7"
            },
        },
        {
            "name": "test_MultiHead.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_MultiHead.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "CUDA_VISIBLE_DEVICES": "7"
            },
        },
        {
            "name": "test_Q16.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_Q16.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "CUDA_VISIBLE_DEVICES": "7"
            },
        },
        {   
            "name": "OpenAI Text Checker",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/jailbreak_diffusion/judger/pre_checker/openai_text_checker.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/jailbreak_diffusion/judger/pre_checker/",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}